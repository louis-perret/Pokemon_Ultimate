<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import java.net.URL?>
<?import javafx.scene.layout.HBox?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vues.controller.FenetreLancement">

        <top>
            <VBox fx:id="vbox" alignment="CENTER" minHeight="100">
                <Text fx:id="t1" smooth="false" stroke="#ffffff00" strokeWidth="0.0" styleClass="text-titre" text="Pokemon Ultimate">
                    <font>
                       <Font name="DejaVu Serif Bold" size="40.0" />
                    </font>
                </Text>
            </VBox>

        </top>
    <center>
        <VBox alignment="CENTER">
            <Button fx:id="boutonStart" minHeight="50" minWidth="150" textFill="indigo" onAction="#lancementJeu">Jouer</Button>
            <Button fx:id="boutonQuitter" minHeight="50" minWidth="150" textFill="indigo" onAction="#quitterJeu">Quitter</Button>
        </VBox>
    </center>


    <stylesheets>
        <URL value="@Lancement.css" />
    </stylesheets>
</BorderPane>

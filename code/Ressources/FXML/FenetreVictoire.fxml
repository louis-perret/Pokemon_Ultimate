<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="vues.controller.FenetreVictoire"
            prefHeight="400.0" prefWidth="600.0">
    <top>
        <VBox alignment="CENTER" minHeight="100">
            <Text smooth="false" stroke="#ffffff00" strokeWidth="0.0" styleClass="text-titre" text="Victoire!" textAlignment="CENTER">
                <font>
                    <Font name="DejaVu Serif Bold" size="23.0" />
                </font>
            </Text>
        </VBox>
    </top>
    <bottom>
        <VBox alignment="CENTER">
            <ImageView fx:id="masterball" fitHeight="300" fitWidth="300" pickOnBounds="true" preserveRatio="true"/>
        </VBox>
    </bottom>


</BorderPane>
